{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "outDir": "js",
    "checkJs": true,
    "allowJs": true,
    "lib": [
      "dom",
      "es2015",
      "es5",
      "es6"
    ],
  },
  "include": [
    "ts/**/*"
  ],
  "exclude": [
    "js",
    "node_modules/**",
    "ts/**/__test__",
  ]
}